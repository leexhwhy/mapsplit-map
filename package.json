{
    "name": "mapsplit-map",
    "version": "0.1.0",
    "description": "Prototype for Leaflet using OpenStreetMap PBF vector tiles generated by Mapsplit",
    "main": "js/app.js",
    "dependencies": {
        "osm-pbf-leaflet": "0.1.0",
        "leaflet-tilelayer-vector": "0.1.0"
    },
    "scripts": {
        "browserify": "browserify . -o bundle.js --bare && browserify -e ./js/worker.js -o bundle-worker.js --bare",
        "watch": "watchify . -o bundle.js --bare -d -v"
    }
}
